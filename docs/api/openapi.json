    "info": {
        "title": "Orynt API",
        "description": "API documentation for the Orynt x402 ai api wrapper",
        "version": "1.0",
        "contact": {}
    },
    "tags": [],
    "servers": [
        {
            "url": "https://api.oryntai.xyz/",
            "description": "Base URL"
        }
    ],
    "components": {
        "schemas": {
            "ChatDto": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string",
                        "description": "Model to use",
                        "example": "gpt-4.1"
                    },
                    "prompt": {
                        "type": "string",
                        "description": "Optional prompt text",
                        "example": "Hello, world!"
                    },
                    "messages": {
                        "description": "Messages array or string input for chat models",
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "input": {
                        "description": "Input text or array for chat/completion",
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "temperature": {
                        "type": "number",
                        "description": "Temperature setting",
                        "minimum": 0,
                        "maximum": 2,
                        "example": 0.7
                    },
                    "max_tokens": {
                        "type": "number",
                        "description": "Maximum tokens for completion",
                        "minimum": 1,
                        "example": 256
                    },
                    "options": {
                        "type": "object",
                        "description": "Provider-specific extra fields"
                    }
                },
                "required": [
                    "model"
                ]
            },
            "EmbeddingDto": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string",
                        "description": "Embedding model to use",
                        "example": "text-embedding-3-small"
                    },
                    "text": {
                        "type": "string",
                        "description": "Text to embed",
                        "example": "Hello world"
                    },
                    "options": {
                        "type": "object",
                        "description": "Provider-specific options"
                    }
                },
                "required": [
                    "model",
                    "text"
                ]
            },
            "ImageDto": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string",
                        "description": "Image generation model",
                        "example": "dall-e-3"
                    },
                    "prompt": {
                        "type": "string",
                        "description": "Prompt for image generation",
                        "example": "A futuristic cityscape at sunset"
                    },
                    "n": {
                        "type": "number",
                        "description": "Number of images to generate",
                        "minimum": 1,
                        "example": 1
                    },
                    "size": {
                        "type": "string",
                        "description": "Image size",
                        "example": "512x512"
                    },
                    "options": {
                        "type": "object",
                        "description": "Provider-specific options"
                    }
                },
                "required": [
                    "model",
                    "prompt"
                ]
            },
            "SttDto": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string",
                        "description": "STT model to use",
                        "example": "whisper-1"
                    },
                    "audioUrl": {
                        "type": "string",
                        "description": "URL of the audio file",
                        "example": "https://example.com/audio.mp3"
                    },
                    "options": {
                        "type": "object",
                        "description": "Provider-specific options"
                    }
                },
                "required": [
                    "model",
                    "audioUrl"
                ]
            },
            "TtsDto": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string",
                        "description": "TTS model to use",
                        "example": "gpt-voice-1"
                    },
                    "text": {
                        "type": "string",
                        "description": "Text to convert to speech",
                        "example": "Hello world"
                    },
                    "voice": {
                        "type": "string",
                        "description": "Voice style or name",
                        "example": "alloy"
                    },
                    "options": {
                        "type": "object",
                        "description": "Provider-specific options"
                    }
                },
                "required": [
                    "model",
                    "text"
                ]
            }
        }
    }
}